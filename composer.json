{
	"name": "barbq/yii2eauth",
	"description": "Yii2 EAuth Extension. EAuth allows to authenticate users with accounts on other websites (Google, Facebook, Twitter, etc).",
	"keywords": ["yii2", "extension", "eauth", "openid", "oauth", "authentication"],
	"homepage": "https://github.com/BarBQ/yii2eauth",
	"type": "yii2-extension",
	"license": "New BSD License",
	"authors": [
		{
			"name": "Alexander Nikitin",
			"email": "nikitin-a-g@yandex.ru",
			"homepage": ""
		}
	],
	"support": {
		"source": "https://github.com/BarBQ/yii2eauth"
	},
	"autoload": {
		"psr-4": {
			"barbq\\eauth\\": "src/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"lib-curl": "*",
		"yiisoft/yii2": "*",
		"lusitanian/oauth": "~0.3.0",
		"nodge/lightopenid": "~1.1.0"
	},
	"extra": {
		"bootstrap": "barbq\\eauth\\Bootstrap"
	}
}
